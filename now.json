{
  "version": 2,
  "env": {
    "AIRTABLE_API_KEY": "@airtable-api-key"
  },
  "builds": [
    { "src": "www/package.json", "use": "@now/next" },
    { "src": "api/*/*.js", "use": "@now/node" }
  ],
  "alias": ["s.bcox.tech"],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/www/$1" }
  ]
}
